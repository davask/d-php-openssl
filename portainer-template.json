[
  {
    "title": "PHP 7.0 + SSL",
    "description": "p7.0 + a2.4 + u16.04 + openssl",
    "logo": "http://www.pngall.com/wp-content/uploads/2016/05/PHP-Logo.png",
    "image": "davask/d-php-openssl:7.0-a2.4-u16.04",
    "ports": [
        "22/tcp",
        "80/tcp",
        "443/tcp"
    ],
    "env": [
        {
            "name": "DWL_LOCAL_LANG",
            "label": "Local lang [en_US:en]"
        },{
            "name": "DWL_LOCAL",
            "label": "Local [en_US.UTF-8]"
        },{
            "name": "DWL_SUDO_USER",
            "label": "Username [username]"
        },{
            "name": "DWL_SSH_ACCESS",
            "label": "Enable ssh access [false]"
        },{
            "name": "DWL_USER_ID",
            "label": "User id [1000]"
        },{
            "name": "DWL_USER_PASSWD",
            "label": "User password for ssh access [secret]"
        },{
            "name": "DWL_HTTP_SERVERADMIN",
            "label": "Server Admin email address [admin@localhost]"
        },{
            "name": "DWL_HTTP_DOCUMENTROOT",
            "label": "Document root [/var/www]"
        }{
            "name": "DWL_HTTP_SHIELD",
            "label": "htpasswd activated [false]"
        }{
            "name": "DWL_SSLKEY_C",
            "label": "Country Name (2 letter code) [EU]"
        }{
            "name": "DWL_SSLKEY_ST",
            "label": "State or Province Name (full name) [France]"
        }{
            "name": "DWL_SSLKEY_L",
            "label": "Locality Name (eg, city) [Vannes]"
        }{
            "name": "DWL_SSLKEY_O",
            "label": "Organization Name (eg, company) [davask web limited - docker container]"
        }{
            "name": "DWL_SSLKEY_CN",
            "label": "Common Name (e.g. server FQDN or YOUR name) [davaskweblimited.com]"
        }{
            "name": "DWL_PHP_DATETIMEZONE",
            "label": "php.ini > date.timezone [Europe/Paris]"
        }
    ],
    "volumes": [
        "/home/host"
    ]
  }
]
